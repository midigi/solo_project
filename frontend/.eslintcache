omments />\n                <WriteComments/>\n                </div>\n                <div className='spacing'/>\n\n                {article.length > 1 && (\n                <div className=\"trending\">\n                    <div className='header2'>Trending Now</div>\n                    {article.slice(4,7).map(art => renderArticle(art))}\n                </div>)}\n            </div>\n        </>\n\n    )\n\n}\n\nexport default ViewArticle;\n","/Users/michaeldigiovanni/AppAcademyResources/Solo_Project/frontend/src/components/NotFound/index.js",[],"/Users/michaeldigiovanni/AppAcademyResources/Solo_Project/frontend/src/components/WriteComments/index.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"85","replacedBy":"89"},{"ruleId":"87","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":13,"column":7,"nodeType":"93","endLine":13,"endColumn":17,"suggestions":"94"},{"ruleId":"95","severity":1,"message":"96","line":16,"column":11,"nodeType":"97","messageId":"98","endLine":16,"endColumn":18},{"ruleId":"95","severity":1,"message":"99","line":24,"column":11,"nodeType":"97","messageId":"98","endLine":24,"endColumn":26},{"ruleId":"91","severity":1,"message":"92","line":38,"column":8,"nodeType":"93","endLine":38,"endColumn":18,"suggestions":"100"},"no-native-reassign",["101"],"no-negated-in-lhs",["102"],["101"],["102"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'article_id'. Either include it or remove the dependency array.","ArrayExpression",["103"],"no-unused-vars","'history' is assigned a value but never used.","Identifier","unusedVar","'sessionSelector' is assigned a value but never used.",["104"],"no-global-assign","no-unsafe-negation",{"desc":"105","fix":"106"},{"desc":"105","fix":"107"},"Update the dependencies array to be: [article_id, dispatch]",{"range":"108","text":"109"},{"range":"110","text":"109"},[456,466],"[article_id, dispatch]",[1236,1246]]